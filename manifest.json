    "description": "Productivity Extension",
    "icons": { 
      "16": "cat16.png",
      "32": "cat32.png",
      "48": "cat48.png",
      "128": "cat128.png" },
    "content_scripts": [
        {
          "matches": ["https://www.youtube.com/*","https://www.reddit.com/*","https://www.instagram.com/*"],
          "js": ["./main.js"]
        }
      ]
}





